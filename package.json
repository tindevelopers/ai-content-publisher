    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.20.5",
    "typedoc": "^0.25.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
