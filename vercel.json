{
  "version": 2,
  "name": "ai-content-publisher-docs",
  "builds": [
    {
      "src": "docs-site/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/docs-site/index.html"
    },
    {
      "src": "/getting-started",
      "dest": "/docs-site/getting-started.md"
    },
    {
      "src": "/api/(.*)",
      "dest": "/docs-site/api/$1"
    },
    {
      "src": "/examples/(.*)",
      "dest": "/docs-site/examples/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/docs-site/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
